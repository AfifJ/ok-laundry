---
import TransactionList from '../../components/TransactionList'
import Layout from '../../layouts/Layout.astro'
---

<Layout title="Daftar Transaksi">
	<main>
		<div class="inline-flex h-96 w-96 flex-col items-center justify-start gap-7 bg-white">
			<div class="w-80 justify-between items-center inline-flex">
        <div class="grow shrink basis-0 text-black text-2xl font-bold">Transaksi</div>
        <div class="grow shrink basis-0 h-11 justify-end items-center gap-2 flex">
          <img class="w-11 h-11 rounded-full" src="https://via.placeholder.com/46x46" />
        </div>
      </div>
			
      <TransactionList client:load />
      
		</div>
	</main>
</Layout>
